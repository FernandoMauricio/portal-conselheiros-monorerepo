{
  "expo": {
    "name": "PortalConselheirosMobile",
    "slug": "portal-conselheiros-mobile",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/react-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/react-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.senac.portalconselheirosmobile",
      "permissions": ["android.permission.CAMERA", "android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/react-logo.png"
    },
    "plugins": [
      "expo-router",
      "react-native-vision-camera",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "targetSdkVersion": 34,
            "compileSdkVersion": 34,
            "kotlinVersion": "1.9.24"
          }
        }
      ]
    ],
    "experiments": { "typedRoutes": true },
    "extra": {
      "router": { "origin": false },
      "eas": { "projectId": "your-eas-project-id" }
    }
  }
}

